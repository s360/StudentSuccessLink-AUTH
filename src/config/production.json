{
  "token": {
    "expires_in": 3600
  },
  "session": {
    "saveUninitialized": true,
    "resave": true
  },
  /**
   * Cross Domain Setting
   */
   "cross": {
        "enable": true,
        "allow_origin": "*",
        "allow_headers": "Authorization, Origin, X-Requested-With, Content-Type, Accept, X-Cbo-Client-Url",
        "allow_method": "POST, GET, PUT, OPTIONS, DELETE"
    },
  "cache": {
        "enable": true,
        "adapter": "memory",
        "backup": "redis",
        "memory": {
            "max": 100,
            "ttl": 60
        }
    }
}