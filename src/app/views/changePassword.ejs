<% layout('layout') -%>
<div class="row  login-container">
    <div class="login-form col-xs-12 col-md-offset-4 col-md-4">
        <div class="col-xs-12 col-md-12">
            <div class="login-page-title text-center"><h1><%= __('ssl') %></h1></div>
        </div>
        <div class="col-xs-12 col-md-12 card ">
            <div class="col-xs-12 col-md-12">
                <div class="title">
                    <h1><%= __('change_password') %></h1>
                    <hr>
                </div>
            </div>
            <div class="col-xs-12 col-md-12">
                <form class="form-horizontal form-signin ng-dirty ng-valid-parse ng-valid ng-valid-required" id="loginForm" name="loginForm" role="form" action="/api/user/changepassword" method="post">
                    <div class="form-group">
                        <div class="col-xs-12 col-md-12">
                            <input type="text" class="form-control ng-dirty ng-valid-parse ng-valid ng-valid-required ng-touched" name="first_name" placeholder="<%= __('first_name') %>" autofocus="autofocus" style="background-image: url(data:image/png;base64,); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;" autocomplete="off">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-xs-12 col-md-12">
                            <input type="text" class="form-control ng-dirty ng-valid-parse ng-valid ng-valid-required ng-touched" name="middle_name" placeholder="<%= __('middle_name') %>" autofocus="" style="background-image: url(data:image/png;base64,); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;" autocomplete="off">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-xs-12 col-md-12">
                            <input type="text" class="form-control ng-dirty ng-valid-parse ng-valid ng-valid-required ng-touched" name="last_name" placeholder="<%= __('last_name') %>" required="true" autofocus="" style="background-image: url(data:image/png;base64,); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;" autocomplete="off">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-xs-12 col-md-12">
                            <input type="password" class="form-control ng-dirty ng-valid-parse ng-valid ng-valid-required ng-touched" name="password" placeholder="<%= __('new_password') %>" required="true" autofocus="" style="background-image: url(data:image/png;base64,); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;" autocomplete="off">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-xs-12 col-md-12">
                            <input type="password" class="form-control ng-untouched ng-dirty ng-valid-parse ng-valid ng-valid-required" name="confirm_password" placeholder="<%= __('confirm_password') %>" required="" style="background-image: url(data:image/png;base64,); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;" autocomplete="off">
                        </div>
                    </div>
                    <div class="form-group">
                        <input type="hidden" name="email" value="<%= session.email %>" />
                        <input type="hidden" name="authCode" value="<%= session.authCode %>" />
                        <input type="hidden" name="redirectTo" value="<%= session.redirectTo %>" />
                        <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                        <button type="submit" value="Submit" class="button"><%= __('change') %></button>
                    </div>
                    <div class="form-group">
                        <div class="col-xs-12 col-md-12">
                            <div class="login-help-link"><%= __('help_change_password_link') %><a href="mailto:help@studentsuccesslink.org"> help@studentsuccesslink.org.</a></div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>